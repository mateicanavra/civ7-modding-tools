{
  "name": "mapgen-studio",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.7",
  "scripts": {
    "predev": "bun run --cwd ../../packages/browser-recipes build",
    "dev": "vite",
    "gen:civ7-tables": "bun ../../scripts/mapgen-studio/generate-civ7-browser-tables.ts",
    "check:worker-bundle": "node scripts/check-worker-bundle.mjs",
    "prebuild": "bun run --cwd ../../packages/browser-recipes build",
    "build": "tsc && vite build && bun run check:worker-bundle",
    "preview": "vite preview"
  },
  "dependencies": {
    "@civ7/adapter": "workspace:*",
    "@deck.gl/core": "^9.0.43",
    "@deck.gl/layers": "^9.0.43",
    "@deck.gl/react": "^9.0.43",
    "@rjsf/core": "^6.2.5",
    "@rjsf/utils": "^6.2.5",
    "@rjsf/validator-ajv8": "^6.2.5",
    "@mapgen/browser-recipes": "workspace:*",
    "@swooper/mapgen-core": "workspace:*",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
