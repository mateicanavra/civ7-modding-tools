{
  "name": "@swooper/mapgen-viz",
  "version": "0.1.0",
  "description": "Shared visualization contract types for MapGen (manifest/protocol/value semantics)",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "check": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.14.0"
  }
}
