{
  "name": "@mapgen/browser-recipes",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./browser-test": {
      "types": "./dist/browser-test.d.ts",
      "import": "./dist/browser-test.js"
    }
  },
  "scripts": {
    "build": "tsup && bun run gen:types",
    "gen:types": "bun ./scripts/generate-types.ts",
    "check": "bun run build"
  },
  "dependencies": {
    "@civ7/adapter": "workspace:*",
    "@swooper/mapgen-core": "workspace:*"
  },
  "devDependencies": {
    "json-schema-to-typescript": "^15.0.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
